{
    "name": "chilean-territorial-divisions",
    "version": "1.0.0",
    "description": "División político-administrativa de Chile: regiones, provincias y comunas",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "test": "vitest run",
        "test:watch": "vitest",
        "prepublishOnly": "npm run build",
        "build:docs": "npm run build && mkdir -p docs && cp dist/index.global.js docs/lib.js"
    },
    "keywords": [
        "chile",
        "regiones",
        "provincias",
        "comunas",
        "territorial",
        "divisions",
        "geography",
        "iso-3166-2"
    ],
    "author": "Hans",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/hsvchcl/chilean-territorial-divisions.git"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.0",
        "vitest": "^1.2.0"
    }
}